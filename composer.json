{
  "name": "tunnelconflux/easy-role",
  "description": "Implementation of permission base role for Laravel by using spatie/laravel-permission",
  "authors": [
    {
      "name": "Abu Bakar Siddique",
      "email": "absiddique.live@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "permission",
    "tunnelconflux",
    "authentication",
    "easy-role",
    "spatie",
    "role"
  ],
  "license": "MIT",
  "require": {
    "spatie/laravel-permission": "^2.37",
    "laravelcollective/html": "^5.8",
    "tunnelconflux/dev-crud": "*"
